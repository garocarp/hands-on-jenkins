#!/usr/bin/groovy
 pipeline {
    agent any
     options {
        disableConcurrentBuilds()
          }
      stages {
        stage('Test') {
            steps {
                withEnv(['PATH+EXTRA=/usr/local/bin:/usr/bin:/sbin:/bin']) 
		    {
                sh 'env' 
                sh 'docker --version'
                  }
	       }
            }
        }
   }
